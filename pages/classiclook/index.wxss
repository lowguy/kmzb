/* pages/product/index.wxss */
@import '../../app.wxss';
.post-list{
  padding-top: 15px;
}
.item{
  width: 50%;
  float: left;
}
.item .img{
  margin:0 5px;
}
.item:nth-child(odd) .img{
  margin-left: 10px;
}
.item:nth-child(even) .img{
  margin-right: 10px;
}
.item image{
  width: 100%;
  height: 170px;
}